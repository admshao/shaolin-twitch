{
  "name": "shaolin-twitch",
  "version": "0.1.2",
  "repository": {
    "type": "git",
    "url": "git://github.com/admshao/shaolin-twitch.git"
  },
  "homepage": "http://nomadtv.com.br/",
  "contributors": [
    "Fabio \"Shaolin\" Madia <admshao@gmail.com>"
  ],
  "scripts": {
    "electron": "electron electron.js --remote-debugging-port=9222"
  },
  "description": "The on-stream graphics used on twitch.tv/shaolindota Broadcast.",
  "license": "Apache-2.0",
  "nodecg": {
    "compatibleRange": "~0.7.0",
    "dashboardPanels": [
      {
        "name": "schedule",
        "title": "Schedule",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "schedule.html"
      },
      {
        "name": "edit-current-run",
        "title": "Edit Current Run",
        "width": 4,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "dialogs/edit-current-run.html"
      },
      {
        "name": "advertisements",
        "title": "Advertisements",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "advertisements.html"
      },
      {
        "name": "checklist",
        "title": "Checklist",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "checklist.html"
      },
      {
        "name": "omnibar",
        "title": "Omnibar",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "omnibar.html"
      },
      {
        "name": "total",
        "title": "Total",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "total.html"
      },
      {
        "name": "edit-total",
        "title": "Edit Total",
        "width": 2,
        "dialog": true,
        "dialogButtons": [
          {
            "name": "save",
            "type": "confirm"
          },
          {
            "name": "cancel",
            "type": "dismiss"
          }
        ],
        "file": "dialogs/edit-total.html"
      },
      {
        "name": "twitter",
        "title": "Twitter",
        "width": 3,
        "headerColor": "#55acee",
        "file": "twitter.html"
      },
      {
        "name": "interview",
        "title": "Interview",
        "width": 4,
        "headerColor": "#2d4e8a",
        "file": "interview.html"
      },
      {
        "name": "nowplaying",
        "title": "Now Playing",
        "width": 2,
        "headerColor": "#2d4e8a",
        "file": "nowplaying.html"
      }
    ],
    "graphics": [
      {
        "file": "index.html",
        "width": 1280,
        "height": 720,
        "singleInstance": true
      },
      {
        "file": "custom_controls/stopwatches/index.html",
        "width": 1280,
        "height": 720
      }
    ]
  },
  "dependencies": {
    "chokidar": "^1.4.2",
    "clone": "^1.0.2",
    "debounce": "^1.0.0",
    "deep-equal": "^1.0.1",
    "emoji": "^0.3.2",
    "lastfm": "^0.9.2",
    "md5-file": "^2.0.4",
    "numeral": "^1.5.3",
    "osc": "^2.0.2",
    "request": "^2.67.0",
    "request-promise": "^1.0.2",
    "rieussec": "^0.3.0",
    "twitter-stream-api": "^0.4.1"
  },
  "devDependencies": {
    "electron-prebuilt": "~1.1.2"
  }
}
